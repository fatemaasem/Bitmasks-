using namespace std;
int main() {
	
	int t;
	cin >> t;
	while (t--) {
		int n, m;
		cin >> n >> m;
		int value = (n | m);
		cout << (value ^ n) + (value ^ m)<<"\n";
	}
	
	return 0;
}
